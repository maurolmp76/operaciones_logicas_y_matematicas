Proceso transforma_tiempo
	//Realiza un algoritmo que permita convertir horas a segundos
	definir horas como numerico;
	definir resultado como numerico;
	Escribir "Ingrese el numero de horas a convertir en segundos";
	Leer horas;
	
	resultado = horas * 3600;
	
	Escribir "La cantidad de horas ingresadas contienen: ",resultado , " segundos";
	
FinProceso
